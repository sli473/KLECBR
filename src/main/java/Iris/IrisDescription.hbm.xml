<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
    <class name="Iris.IrisDescription" table="Iris">

        <id name="_caseId" column="caseId">
            <generator class="native"/>
        </id>
        <property name="_sepalLengthCm" column="SepalLengthCm" />
        <property name="_sepalWidthCm" column="SepalWidthCm" />
        <property name="_petalLengthCm" column="PetalLengthCm" />
        <property name="_petalWidthCm" column="PetalWidthCm" />

    </class>
</hibernate-mapping>