<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
<class name="MushroomDescription" table="Mushrooms">

	<id name="_caseId" column="caseId">
	  <generator class="native"/>
	</id>
	<property name="_capShape" column="capshape" />
	<property name="_capSurface" column="capsurface"/>
	<property name="_capColour" column="capcolor"/>
	<property name="_bruises" column="bruises"/>
	<property name="_odor" column="odor"/>
	<property name="_gillAttachment" column="gillattachment"/>
	<property name="_gillSpacing" column="gillspacing"/>
	<property name="_gillSize" column="gillsize"/>
	<property name="_gillColor" column="gillcolor"/>
	<property name="_stalkShape" column="stalkshape"/>
	<property name="_stalkRoot" column="stalkroot"/>
	<property name="_stalkSurfaceAboveRing" column="stalksurfaceabovering"/>
	<property name="_stalkColourAboveRing" column="stalkcolorabovering"/>
	<property name="_veilType" column="veiltype"/>
	<property name="_veilColour" column="veilcolor"/>
	<property name="_ringNumber" column="ringnumber"/>
	<property name="_ringType" column="ringtype"/>
	<property name="_sporePrintColor" column="sporeprintcolor"/>
	<property name="_population" column="population"/>
	<property name="_habitat" column="habitat"/>


</class>
</hibernate-mapping>